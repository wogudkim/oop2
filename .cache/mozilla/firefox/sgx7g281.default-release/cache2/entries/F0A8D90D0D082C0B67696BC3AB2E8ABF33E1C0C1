			$('#id_modstandardgrade').removeClass('collapsed containsadvancedelements');
			$('#id_layouthdr').removeClass('collapsed containsadvancedelements');
			$('#id_interactionhdr').removeClass('collapsed containsadvancedelements');
			$('#id_reviewoptionshdr').removeClass('collapsed containsadvancedelements');
